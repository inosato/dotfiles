# Accounts will inherit settings from this section
defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

# Gmail
account gmail
host smtp.gmail.com
port 587
from akamchoiw@gmail.com
user akamchoiw@gmail.com

# A provider's service
account wongdev
host c2s3-1e-syd.hosting-services.net.au
port 587
from andrew@wongdev.com
user andrew@wongdev.com

# Set a default account
account default : gmail

