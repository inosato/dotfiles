# 
# ~/.backup.list
#

# Include
+ /dev/console
+ /dev/initctl
+ /dev/null
+ /dev/zero

# Exclude
- /dev/*
- /run/*
- /proc/*
- /sys/*
- /tmp/*
- lost+found/*
- /media/*
- /mnt/*
- /var/lib/pacman/sync/*

- /home/w0ng/.db-container/*
- /home/w0ng/.mozilla/firefox/*.default/Cache/*
- /home/w0ng/.mozilla/firefox/*.default/OfflineCache/*

# location of backup folder
- /mnt/data0/backup/*
