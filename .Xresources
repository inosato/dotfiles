!
! ~/.Xresources
!

! X Settings
Xft.dpi: 96
Xcursor.theme: Neutral

! Colour Scheme
!#include "/home/w0ng/.config/termcolours/jellybeans"
#include "/home/w0ng/.config/termcolours/hybrid"

! URxvt Options
URxvt.scrollstyle: rxvt
URxvt.scrollBar: off
URxvt.urgentOnBell: True
URxvt.visualBell: True
URxvt.backgroundPixmap: /home/w0ng/pictures/wallpapers/noisebg.png\;style=tiled
! bold, italic, underline
URxvt.colorBD: #B5BD68
URxvt.colorIT: #B294BB
URxvt.colorUL: #81A2BE
! font
URxvt.font: -*-terminuspowerline-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.boldFont: -*-terminuspowerline-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.italicFont: -*-terminuspowerline-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.boldItalicFont: -*-terminuspowerline-medium-r-*-*-12-*-*-*-*-*-*-*
! extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
! clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
