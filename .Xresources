!
! ~/.Xresources
!

! X Settings
Xft.dpi: 96
Xcursor.theme: Vanilla-DMZ

! Colour Scheme
#include "/home/w0ng/.config/termcolours/jellybeans"
!#include "/home/w0ng/.config/termcolours/hybrid"

! URxvt Options
URxvt.scrollstyle: rxvt
URxvt.scrollBar: off
URxvt.urgentOnBell: True
URxvt.visualBell: True
! bold, italic, underline
URxvt.colorBD: #e8e8d3
URxvt.colorIT: #799D6A
URxvt.colorUL: #8197BF
! font
URxvt.font: -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.boldFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.italicFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
URxvt.boldItalicFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
! extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
! clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
