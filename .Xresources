!
! ~/.Xresources
!

! X Settings
Xft.dpi: 96
Xcursor.theme: Neutral

! Colour Scheme
#include "/home/w0ng/.config/termcolours/jellybeans"
!#include "/home/w0ng/.config/termcolours/hybrid"

! URxvt Options
URxvt.depth: 32
URxvt.scrollstyle: rxvt
URxvt.scrollBar: off
URxvt.urgentOnBell: True
URxvt.visualBell: True
URxvt.cursorColor: #00FF00
! bold, italic, underline
URxvt.colorBD: #99AD6A
URxvt.colorIT: #C6B6EE
URxvt.colorUL: #8197BF
! font
URxvt.font: -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-*, xft:DejaVu\ Sans\ Mono
URxvt.boldFont:  -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-*, xft:DejaVu\ Sans\ Mono
URxvt.italicFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-*, xft:DejaVu\ Sans\ Mono
URxvt.boldItalicFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-*, xft:DejaVu\ Sans\ Mono
! extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
! clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
