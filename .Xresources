!
! ~/.Xresources
!

! X Settings
Xft.dpi: 96
Xcursor.theme: Vanilla-DMZ

! Colour Scheme
!#include "/home/w0ng/.config/termcolours/jellybeans"
#include "/home/w0ng/.config/termcolours/hybrid"

! URxvt Options
URxvt.scrollstyle: rxvt
URxvt.scrollBar: off
URxvt.urgentOnBell: True
URxvt.visualBell: True
! bold, italic, underline
URxvt.colorBD: #e8e8d3
URxvt.colorIT: #b5bd68
URxvt.colorUL: #81a2be
! font
URxvt.font: xft:inconsolata_g_Powerline:size=10.5
URxvt.boldFont: xft:Inconsolata_g_Powerline:size=10.5
URxvt.italicFont: xft:Inconsolata_g_Powerline:size=10.5
URxvt.boldItalicFont: xft:Inconsolata_g_Powerline:size=10.5
! extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
! clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
