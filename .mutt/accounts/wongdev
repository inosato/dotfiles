  set from      = "andrew@devwong.com"
  set sendmail  = "/usr/bin/msmtp -a wongdev"
  set mbox      = "+wongdev/inbox"
unset record
  set postponed     = "+wongdev/draft"
#  set query_command = "mail-query '%s' ~/Mail/Gmail/INBOX"
  set signature     = ~/.mutt/accounts/wongdev_sig
  set pgp_sign_as=0x89285DCF

color status magenta default

